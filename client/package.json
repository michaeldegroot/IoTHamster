{
  "name": "reactron-boilerplate",
  "description": "desc",
  "version": "0.0.1",
  "main": "public/electron.js",
  "private": true,
  "dependencies": {
    "app-root-path": "^3.0.0",
    "bootstrap": "^4.4.1",
    "custom-electron-titlebar": "^3.2.1",
    "electron-is-dev": "^1.1.0",
    "electron-util": "^0.13.1",
    "electron-window-state": "^5.0.3",
    "glob": "^7.1.6",
    "knex": "^0.20.8",
    "mysql": "^2.17.1",
    "mysql2": "^2.1.0",
    "node-sass": "^4.13.1",
    "objection": "^2.1.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-icons": "^3.8.0",
    "react-markdown": "^4.3.1",
    "react-particles-js": "^2.7.0",
    "react-router-dom": "^5.1.2",
    "sqlite3": "^4.1.1"
  },
  "engines": {
    "node": ">=13.0.0"
  },
  "author": {
    "name": "GiveMeAllYourCats",
    "email": "specamps@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/michaeldegroot/reactron-boilerplate.git"
  },
  "bugs": {
    "url": "https://github.com/michaeldegroot/reactron-boilerplate/issues",
    "email": "specamps@gmail.com"
  },
  "keywords": [
    "boilerplate",
    "react",
    "electron"
  ],
  "license": "MIT",
  "watch": {
    "electron-dev": {
      "patterns": [
        "**/*"
      ],
      "ignore": "src",
      "extensions": [
        "*.js",
        ".html",
        ".*"
      ],
      "inherit": true,
      "quiet": false,
      "legacyWatch": true,
      "delay": 10
    }
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "test": "mocha",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "migrate:latest": "npx knex migrate:latest",
    "migrate:rollback": "npx knex migrate:rollback",
    "electron": "electron .",
    "lint": "eslint src/**/*.js src/**/*.jsx",
    "electron-dev": "electron . --inspect",
    "dev": "concurrently --kill-others \"npm-watch electron-dev\" \"npm run start\"",
    "postinstall": "electron-builder install-app-deps"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "build": {
    "appId": "electron.boilerplate",
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    },
    "mac": {
      "category": "electron.boilerplate"
    }
  },
  "homepage": "./",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "browserslist": "^4.8.5",
    "concurrently": "^5.0.2",
    "electron": "^9.4.0",
    "electron-builder": "^22.2.0",
    "electron-devtools-installer": "^2.2.4",
    "eslint": "^6.8.0",
    "eslint-plugin-react": "^7.18.0",
    "mocha": "^7.0.0",
    "npm-watch": "^0.6.0",
    "react-scripts": "^3.3.0",
    "spectron": "^9.0.0"
  }
}
